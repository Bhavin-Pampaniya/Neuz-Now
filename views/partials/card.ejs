<% articles.forEach(article=> { %>
<div class="col-md-4 my-4">
  <div class="card" style="width: 22rem; height: 100%">
    <img
    src="<%=article.urlToImage?article.urlToImage:"https://img.etimg.com/thumb/msid-95524313,width-1070,height-580,imgsize-121724,overlay-economictimes/photo.jpg"
    %> " class="card-img-top" alt="..." />
    <div class="card-body">
      <h5 class="card-title">
        <%= article.title? article.title.slice(0,50):"OOPS!! API error" %>...
      </h5>
      <p class="card-text">
        <%= article.description ? article.description.slice(0,120):"OOPS!! API error" %>...
      </p>
      <a href="<%= article.url %>" class="btn btn-primary" target="_blank"
        >Read More...</a>
    </div>
  </div>
</div>
<% }) %>
